{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.2.0",
        "updateMode": "selective",
        
        "example4": {
        "class": "Tenant",
        "A1": {
            "class": "Application",
 
            "example4-ecv": {
                "class": "Monitor",
                "interval": 5,
                "monitorType": "tcp",
                "timeout": 4
            },
            "example4-pool": {
                "class": "Pool",
                "members": [
                    {
                        "servicePort": 80,
                        "serverAddresses": [
                    "10.0.4.100",
                    "10.0.4.101"
                        ],
                        "shareNodes": true
                    }
                ],
                "monitors": [
                    {
                        "use": "example4-ecv"
                    }
                ]
            },
            "example4": {
                "class": "Service_TCP",
                "pool": "example4-pool",
                "profileTCP": {
                    "bigip": "/Common/f5-tcp-progressive"
                },
                "profileUDP": {
                    "bigip": ""
                },
                "remark": "LAYER4 listener for tcp 8443 tcp 8001",
                "snat": {
                    "use": "snatpool-10.0.4.10"
                },
                "virtualAddresses": [
                    "10.0.4.10"
                ],
                "virtualPort": 80
            },
            "snatpool-10.0.4.10": {
                "class": "SNAT_Pool",
                "snatAddresses": [
                    "10.0.4.10"
                ]
            }
    }}
}
}

